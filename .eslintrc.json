{
    "env": {
        "node": true,
        "mocha": true,
        "es6": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "prettier/prettier":["error", {"endOfLine":"auto"}],
        "semi": "error"
    },
    "plugins": [
        "prettier"
    ]
}
